{
    "builds": [
        {
            "src":"api/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"api/app.py"
        }
    ],

    "functions": {
        "food_nurition/api/**/*": {
        "maxDuration": 300
        }
    }

}